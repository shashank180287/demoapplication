		createString = "CREATE TABLE VISITOR_ENQUIRY_INFO ("
				+ "id BIGINT primary key," 
				+ "name varchar(50),"
				+ "mobile_number BIGINT,"
				+ "address varchar(255)," 
				+ "visit_purpose varchar(100),"
				+ "date date,"
				+ "comment varchar(255)" 
				+ ")";
		stmt = conn.createStatement();
		stmt.executeUpdate(createString);
